# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
#end_of_line = lf
charset = utf-8
tab_width   = 4
indent_size = 2
indent_style = tab
#trim_trailing_whitespace = true
#insert_final_newline = true

#[CHANGELOG.md]
#indent_size = 8

[*.{html,js}]
indent_size = 2
indent_style = space
