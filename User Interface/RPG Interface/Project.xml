<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="RPG Interface" file="RPGInterface" main="Main" version="0.0.0" company="Level Up Labs"/>
	
	<window width="800" height="600" fps="30" orientation="landscape" fullscreen="false"/>
 	
	<set name="SWF_VERSION" value="11.8" />
	<set name="BUILD_DIR" value="export" />
	
	<classpath name="source" />
	<assets path="assets" />
	
	<haxelib name="flixel"/>
	<haxelib name="flixel-ui"/>
	<haxelib name="flixel-addons"/>
	<haxelib name="firetongue"/>
	<haxelib name="task"/>

	<haxedef name="FLX_BMP_DEBUG"/>
	<haxedef name="FLX_NO_DEBUG" unless="debug"/>
</project>